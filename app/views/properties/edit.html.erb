
<div class="container">
  <h2>Edit Property</h2>
  <div class="row">
    <div class="col-sm-6">
      <%= form_for @property do |f| %>
        <div class="form-group">
          <%= f.label :name, class: 'col-sm-2 control-label' %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
        <%= f.submit 'Save', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>